aobscanmodule(Health,GameAssembly.dll,F3 0F 11 40 18 EB 03 89 4E) // should be unique
alloc(newmem,$50,Health)

label(code)
label(return)

newmem:
  pushfq
  mov rdi,[rsi+A0] // Dereferencing pointer to isPlayer
  mov rdi,[rdi+98] // Last dereference to isPlayer (either 0 or 1)
  cmp rdi,0        // Checks  whether  damage taken by player
  je code          // If enemy jump to original code
  movss xmm0,[rax+1C] // mov Player Max Health into xmm0
  xor rdi,rdi      // Clear rdi register to restore to 0
code:
  movss [rax+18],xmm0 //mov operation to move updated health value after taking damage
  popfq
  jmp return

Health:
  jmp newmem
return:
registersymbol(Health)

