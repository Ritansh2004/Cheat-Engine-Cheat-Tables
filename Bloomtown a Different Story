  je InstantKill
  jmp code

@@:
  cmp dword ptr [GodMode_Flag],1
  je GodMode

code:
  sub edx,edi
  mov rcx,r12
  jmp return

GodMode:
  xor edi,edi
  jmp code
InstantKill:
  xor edx,edx
  jmp code

HP_Calc_AOB:
  jmp newmem
return:
