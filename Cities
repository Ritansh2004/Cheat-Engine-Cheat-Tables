aobscan(getEcoMan,48 8B 97 e8 00 00 00 48 63 45) // should be unique
alloc(newmem,$1000,getEcoMan)

alloc(ecoman,4)
registersymbol(ecoman)
label(code)
label(return)

newmem:

code:
  mov [ecoman],rdi
  mov rdx,[rdi+000000E8]
  jmp return

getEcoMan:
  jmp newmem
  nop 2
return:
registersymbol(getEcoMan)

[DISABLE]

getEcoMan:
  db CC 8B 97 E8 00 00 00

unregistersymbol(ecoman)
unregistersymbol(getEcoMan)
dealloc(newmem)
dealloc(ecoman)

